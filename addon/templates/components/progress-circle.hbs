<svg
  class="progress-circle__svg"
  width="{{size}}"
  height="{{size}}"
  viewBox="-{{strokeWidth}} -{{strokeWidth}} {{size}} {{size}}"
  xmlns="http://www.w3.org/2000/svg"
>

  <circle
    class="progress-circle__circle"
    class=""
    r="{{radius}}"
    cy="{{XYcoordinates}}"
    cx="{{XYcoordinates}}"
    stroke-width="{{strokeWidth}}"
    fill="none"
  ></circle>

  <circle
    class="{{indicatorClassName}}"
    r="{{radius}}"
    cy="{{XYcoordinates}}"
    cx="{{XYcoordinates}}"
    stroke-width="{{strokeWidth}}"
    fill="none"
    stroke-dasharray="{{circumference}}"
    stroke-dashoffset="{{strokeDashoffset}}"
  >
  </circle>

  {{yield}}

</svg>
